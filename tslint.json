{
    "defaultSeverity": "error",
    "extends": "tslint:recommended",
    "jsRules": {},
    "rules": {
        "prefer-const": true,
        "trailing-comma": [ false ],
        "no-console": false,
        "member-access": [
            true,
            "no-public"
          ],
        "quotemark": [
            true,
            "single",
            "avoid-escape"
        ],
        "object-literal-sort-keys": false,
        "no-var-requires": false
    },
    "rulesDirectory": []
}
